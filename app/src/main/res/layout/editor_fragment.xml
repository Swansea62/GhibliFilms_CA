    <!-- -->

    <!-- Release Date Layout -->

    <TextView
        android:id="@+id/runningTimeHeading"
        android:layout_width="150dp"
        android:layout_height="wrap_content"

        android:layout_marginStart="16dp"
        android:layout_marginTop="496dp"
